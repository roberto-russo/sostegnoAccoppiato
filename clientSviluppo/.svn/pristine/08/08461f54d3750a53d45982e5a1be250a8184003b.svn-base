package it.csi.demetra.demetraws.zoo.compositeIds;

import java.io.Serializable;

public class Logger_id  implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -4381086099452384189L;

	private long idSessione;
	private Long idLog;
	
	
	public Logger_id() {
		super();
	}
	
	public Logger_id(long idSessione, Long idLog) {
		super();
		this.idSessione = idSessione;
		this.idLog = idLog;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((idLog == null) ? 0 : idLog.hashCode());
		result = prime * result + (int) (idSessione ^ (idSessione >>> 32));
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Logger_id other = (Logger_id) obj;
		if (idLog == null) {
			if (other.idLog != null)
				return false;
		} else if (!idLog.equals(other.idLog))
			return false;
		if (idSessione != other.idSessione)
			return false;
		return true;
	}
}
