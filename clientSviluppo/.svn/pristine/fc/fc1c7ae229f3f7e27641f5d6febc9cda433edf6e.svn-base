package it.csi.demetra.demetraws.zoo.controlli.visitor;

import it.csi.demetra.demetraws.zoo.controlli.visitor.entityRef.Controllo;
import it.csi.demetra.demetraws.zoo.controlli.visitor.entityRef.Ref02_004;
import it.csi.demetra.demetraws.zoo.controlli.visitor.entityRef.Ref02_005;
import it.csi.demetra.demetraws.zoo.model.Dmt_s_sessione;
import it.csi.demetra.demetraws.zoo.model.Rpu_V_pratica_zoote;
import it.csi.demetra.demetraws.zoo.services.ControlliService;

public class EntityFactory {

    public EntityFactory() {
    }

    public Controllo getVisitable(Rpu_V_pratica_zoote azienda, Dmt_s_sessione sessione, ControlliService controlliService, Rpu_V_pratica_zoote rpu_v_pratica_zoote) throws ControlloException {

        switch (azienda.getCodicePremio()) {
            case "311":
                return new Ref02_005(sessione, controlliService, azienda);

            case "313":
                return new Ref02_004(sessione, controlliService, azienda);

            default:
                throw new ControlloException("CODICE PREMIO NON ELABORATO");
        }
    }

}
