package it.csi.demetra.demetraws.zoo.controlli.visitor;

import it.csi.demetra.demetraws.zoo.controlli.visitor.entityRef.Ref02_004;
import it.csi.demetra.demetraws.zoo.controlli.visitor.entityRef.Ref02_005;
import it.csi.demetra.demetraws.zoo.model.Rpu_V_pratica_zoote;

public class EntityFactory {

    public EntityFactory() {
    }

    public ControlloVisitable getVisitable(Rpu_V_pratica_zoote azienda) {
        ControlloVisitable res = null;

        if(azienda.getCodicePremio().equals("310"))
            res = new Ref02_004();

        if(azienda.getCodicePremio().equals("311"))
            res = new Ref02_005();

        return res;
    }

}
